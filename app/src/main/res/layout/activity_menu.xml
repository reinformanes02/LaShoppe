<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:background="@color/simplebackgroundcolor"
    tools:context=".Menu">


    <ImageView
        android:id="@+id/imageView_logo"
        android:layout_width="413dp"
        android:layout_height="474dp"
        android:alpha="0.2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout"
        app:srcCompat="@drawable/logo" />

    <ImageView
        android:id="@+id/logo"
        android:layout_width="60dp"
        android:layout_height="44dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="328dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <TextView
        android:id="@+id/logoname"
        android:layout_width="183dp"
        android:layout_height="36dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="144dp"
        android:text="La Shoppe"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/textView5"
        android:layout_width="166dp"
        android:layout_height="36dp"
        android:layout_marginTop="64dp"
        android:text="Recommended"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.093"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logo" />

    <ImageView
        android:id="@+id/profileicon"
        android:layout_width="41dp"
        android:layout_height="38dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/logoname"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/profile" />

    <ImageView
        android:id="@+id/search_icon"
        android:layout_width="32dp"
        android:layout_height="36dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="23dp"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/profileicon"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/searchicon" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="408dp"
        android:layout_height="51dp"
        android:layout_marginTop="8dp"
        android:backgroundTint="#00FFFFFF"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logoname"
        tools:visibility="visible">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/vertical_sellId"
            android:layout_width="156dp"
            android:layout_height="38dp"
            android:layout_marginStart="36dp"
            android:layout_marginBottom="7dp"
            android:background="@drawable/rounded_white_background">

            <CheckBox
                android:id="@+id/checkbox_sell"
                android:layout_width="33dp"
                android:layout_height="36dp"
                android:background="@android:drawable/checkbox_on_background"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.142"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="81dp"
                android:layout_height="34dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="8dp"
                android:clickable="true"
                android:text="Sell"
                android:textAlignment="textStart"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.333"
                app:layout_constraintStart_toEndOf="@+id/checkbox_sell"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0" />

        </androidx.constraintlayout.widget.ConstraintLayout>


        <View
            android:id="@+id/spacer_view"
            android:layout_width="42dp"
            android:layout_height="match_parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"></View>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout2"
            android:layout_width="156dp"
            android:layout_height="38dp"
            android:layout_marginBottom="64dp"
            android:background="@drawable/rounded_white_background">


            <TextView
                android:id="@+id/text_categories"
                android:layout_width="81dp"
                android:layout_height="33dp"
                android:layout_marginTop="4dp"
                android:layout_marginEnd="16dp"
                android:gravity="center"
                android:text="Categories"
                android:textColor="@android:color/black"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/icon_categories"
                android:layout_width="39dp"
                android:layout_height="34dp"
                android:layout_marginTop="3dp"
                android:layout_marginEnd="9dp"
                android:layout_marginBottom="2dp"
                android:contentDescription="Categories Icon"
                android:src="@drawable/categories"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/text_categories"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="1.0" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/display2"
        android:layout_width="177dp"
        android:layout_height="180dp"
        android:layout_marginTop="36dp"
        android:alwaysDrawnWithCache="false"
        android:background="@drawable/rounded_white_background"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.404"
        app:layout_constraintStart_toEndOf="@+id/display1"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout">

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="117dp"
            android:layout_height="125dp"
            android:layout_gravity="center"
            app:srcCompat="@drawable/_23600153_774881284125466_5259258152897573738_n" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="Phone Case"
            android:textAlignment="center"
            android:textColor="#040404"
            android:textStyle="bold" />

        <Button
            android:id="@+id/item2"
            android:layout_width="wrap_content"
            android:layout_height="29dp"
            android:layout_gravity="center"
            android:background="@drawable/round_green_button"
            android:text="view"
            android:textColor="#FFFFFF" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/display1"
        android:layout_width="177dp"
        android:layout_height="180dp"
        android:layout_marginStart="10dp"
        android:alwaysDrawnWithCache="false"
        android:background="@drawable/rounded_white_background"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5">

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="86dp"
            android:layout_height="114dp"
            android:layout_gravity="center"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/_23599881_1065910604632210_3062875474689588846_n" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="Shoes"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textStyle="bold" />

        <Button
            android:id="@+id/item1"
            android:layout_width="wrap_content"
            android:layout_height="34dp"
            android:layout_gravity="center"
            android:background="@drawable/round_green_button"
            android:text="view"
            android:textColor="#FFFFFF" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/display4"
        android:layout_width="177dp"
        android:layout_height="180dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/rounded_white_background"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="@+id/imageView_logo"
        app:layout_constraintHorizontal_bias="0.105"
        app:layout_constraintStart_toEndOf="@+id/display3"
        app:layout_constraintTop_toBottomOf="@+id/display2">

        <ImageView
            android:id="@+id/imageView15"
            android:layout_width="125dp"
            android:layout_height="99dp"
            android:layout_gravity="center"
            android:layout_weight="1"
            app:srcCompat="@drawable/_23686749_711457387826729_3490164860791250226_n" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:text="Clothes"
            android:textAlignment="center"
            android:textColor="#1E1D1D"
            android:textStyle="bold" />

        <Button
            android:id="@+id/item4"
            android:layout_width="wrap_content"
            android:layout_height="15dp"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:background="@drawable/round_green_button"
            android:text="view"
            android:textAlignment="center"
            android:textColor="#FFFFFF" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/display5"
        android:layout_width="177dp"
        android:layout_height="180dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/rounded_white_background"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/display3">

        <ImageView
            android:id="@+id/imageView16"
            android:layout_width="163dp"
            android:layout_height="112dp"
            android:layout_gravity="center"
            android:layout_weight="1"
            app:srcCompat="@drawable/_23735692_2577805542380874_6328510971373479716_n" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:text="Jacket"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textStyle="bold" />

        <Button
            android:id="@+id/item5"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:background="@drawable/round_green_button"
            android:text="view"
            android:textColor="#FAF5F5" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/display3"
        android:layout_width="177dp"
        android:layout_height="180dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/rounded_white_background"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/display1">

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="126dp"
            android:layout_height="119dp"
            android:layout_gravity="center"
            android:layout_weight="1"
            app:srcCompat="@drawable/_23619404_736004511635797_1241129639976856689_n" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="95dp"
            android:layout_height="21dp"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:text="Bag"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="16sp"
            android:textStyle="bold" />

        <Button
            android:id="@+id/item3"
            android:layout_width="99dp"
            android:layout_height="33dp"
            android:layout_gravity="center"
            android:background="@drawable/round_green_button"
            android:text="view"
            android:textColor="#FFFFFF" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/iconDown"
        android:layout_width="398dp"
        android:layout_height="43dp"
        android:layout_marginEnd="4dp"
        android:background="@drawable/rounded_gray_spinner_background"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/home1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:srcCompat="@drawable/home" />

        <ImageView
            android:id="@+id/cart"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:srcCompat="@drawable/cart" />

        <ImageView
            android:id="@+id/profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:srcCompat="@drawable/profile" />

    </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>